package com.altruist.response;

import com.altruist.nsdl.model.UserTokenDetailModel;

public class LoginResponse {

	private StatusDescription statusDescription;
	private UserTokenDetailModel userTokenDetail;

	public StatusDescription getStatusDescription() {
		return statusDescription;
	}

	public void setStatusDescription(StatusDescription statusDescription) {
		this.statusDescription = statusDescription;
	}

	public UserTokenDetailModel getUserTokenDetail() {
		return userTokenDetail;
	}

	public void setUserTokenDetail(UserTokenDetailModel userTokenDetail) {
		this.userTokenDetail = userTokenDetail;
	}

	@Override
	public String toString() {
		return "LoginResponse [statusDescription=" + statusDescription + ", userTokenDetail=" + userTokenDetail + "]";
	}

}
